{
  "name": "shortify-cli",
  "version": "0.0.1",
  "description": "An interactive CLI to shorten URLs on the web",
  "private": "true",
  "main": "dist/index.js",
  "types": "dist/index.js",
  "bin": {
    "url": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "link-cli": "npm unlink shortify-cli && chmod +x dist/index.js && npm link shortify-cli"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halvaradop/ts-cli.git"
  },
  "keywords": [
    "typescript",
    "cli",
    "terminal",
    "commander",
    "interactive",
    "shortener",
    "url"
  ],
  "author": "Hernan Alvarado <hernanvid123@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halvaradop/ts-cli/issues"
  },
  "homepage": "https://github.com/halvaradop/ts-cli#readme",
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "dotenv": "^16.4.5",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist"
  ]
}
